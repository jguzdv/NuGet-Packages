{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ApiBaseUrl": "https://localhost:4999/",

  "ReverseProxy": {
    "Proxies": [
      {
        "PathMatch": "/api/{**catch-all}",
        "UpstreamUrl": "https://localhost:4999/",
        "PathPrefix": "/api",
        "UseAccessToken": true
      }
    ]
  },

  // Alteratively - Remotely managed Features
  "Features": "https://localhost:4999/_app/feature"
}
